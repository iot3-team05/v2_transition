iot3: 2.0.0
info:
  name: '[Compact] CommScope Ion-M'
  oem: CommScope
  model: Ion-M
  versions:
    - "7.4"
  author: namira.mansuri@qeleo.com
  stage: alpha
  revision: 3
  labels:
    - commscope
    - ionm
    - compact
    - auto upgraded to v2
  created: "2021-05-09"
  updated: "2022-12-06"
  documentation:
    description: This file supports the alarms of system CommScope Ion-M
registration:
  vaultId: vault_id
  fingerprint:
    - ip
    - port
    - version
    - vault_id
  vault:
    properties:
      password:
        key: password
        placeholder: Password
        required: true
        title: Password
        type: string
      username:
        key: username
        placeholder: Username
        required: true
        title: Username
        type: string
    required:
      - username
      - password
  form:
    properties:
      ip:
        key: ip
        placeholder: IP Address
        required: true
        title: IP
        type: string
      port:
        inputType: number
        key: port
        maximum: 65535
        minimum: 1
        placeholder: Port
        required: true
        title: Port
        type: number
      vault_id:
        key: vault_id
        pattern: ^(cu\w{6})(-av\w{6})$
        placeholder: Vault ID
        required: true
        title: Vault ID
        type: string
      version:
        key: version
        placeholder: Version
        required: true
        title: Version
        type: string
    required:
      - ip
      - port
      - vault_id
      - version
engines:
  tunnel:
    ssh:
      supported: false
    http:
      supported: true
  upgraded-http:
    schedule: '@always'
    auth: '%{resources.auths.upgraded-http}'
    type: HTTP
    options:
      tls:
        minVersion: "1.0"
topology:
  rootType: mc
  nodes: true
  discovery:
    dataEngine: '%{engines.upgraded-http}'
    dataSource: '%{resources.http.requests.get_inventory}'
    type: http
    expression: {}
    selectors:
      nodes:
        dataEngine: '%{engines.upgraded-http}'
        selector:
          type: regexp
          expression: "/(?<=[[])(?!'ION\b)(.*?)(?=[,]]|,,)/g"
    flatten:
      type:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=).*?(?=,))/ => $1
          2: |
            switch
            "%{1}" ~= "Master Controller" : "mc"
            "%{1}" ~= "TSUN" : "tsun"
            "%{1}" ~= "TFAH" : "tfah"
            "%{1}" ~= "TFLN" : "tfln"
            "%{1}" ~= "TPRN" : "tprn"
            "%{1}" ~= "SAB" : "sab"
            "%{1}" ~= "PDU" : "pdu"
            "%{1}" ~= "POI - SR" : "poi_sr"
            "%{1}" ~= "i-POI" : "i_poi"
            "%{1}" ~= "IFC" : "ifc"
            default: ""
      topology:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=).*?(?=,))/ => $1
          2: |
            switch
            "%{1}" ~= "Master Controller" : "mc"
            "%{1}" ~= "TSUN" : "tsun"
            "%{1}" ~= "TFAH" : "tfah"
            "%{1}" ~= "TFLN" : "tfln"
            "%{1}" ~= "TPRN" : "tprn"
            "%{1}" ~= "SAB" : "sab"
            "%{1}" ~= "PDU" : "pdu"
            "%{1}" ~= "POI - SR" : "poi_sr"
            "%{1}" ~= "i-POI" : "i_poi"
            "%{1}" ~= "IFC" : "ifc"
            default: ""
          3: use %{0}
          4: regexpExtract /((?<=addr[=])\d+)/ => $1
          5: |
            switch
            "%{4}" ~= "^[1-8]$" : "1"
            "%{4}" ~= "^[9]|[1][0-6]$" : "2"
            "%{4}" ~= "^[1][7-9]|[2][0-4]$" : "3"
            "%{4}" ~= "^[2][5-9]|[3][0-2]$" : "4"
            "%{4}" ~= "^[3][3-9]|[4][0]$" : "5"
            "%{4}" ~= "^[4][0-8]$" : "6"
            "%{4}" ~= "^[4][9]|[5][0-6]$" : "7"
            "%{4}" ~= "^[5][7-9]|[6][0-4]$" : "8"
            "%{4}" ~= "^[6][5-9]|[7][0-2]$" : "9"
            "%{4}" ~= "^[7][3-9]|[8][0]$" : "10"
            "%{4}" ~= "^[8][1-8]$" : "11"
            default: ""
          6: use %{1}
          7: regexpExtract /(\d+$|\d+[.]\d+[.]\d+|\d+[.]\d+)/ => $1
          8: regexpExtract /^(\d+)/ => $1
          9: use %{7}
          10: regexpExtract /((?<=\.)\d+(?=\.))/ => $1
          11: use %{7}
          12: regexpExtract /(\d+)$/ => $1
          13: use %{7}
          14: regexpReplace /^(\d+\.\d\.)/ => ''
          15: |
            switch
            "%{2}" == "mc" : "'MC'"
            "%{2}" == "pdu" : "'MC' -> 'PDU [%{8}]'"
            "%{2}" == "poi_sr" : "'MC' -> 'POI-SR [%{8}]'"
            "%{2}" == "ifc" : "'MC' -> 'POI-SR [%{4}]' -> 'IFC'"
            "%{2}" == "i_poi" : "'MC' -> 'POI-SR [%{5}]' -> 'I-POI [%{12}]'"
            "%{2}" == "tprn" : "'MC' -> 'TPRN [%{8}]'"
            "%{2}" == "tfln" : "'MC' -> 'TPRN [%{8}]' -> 'TFLN [%{12}]'"
            "%{2}" == "tfah" : "'MC' -> 'TPRN [%{8}]' -> 'TFLN [%{10}]' -> 'TFAH [%{12}]'"
            "%{2}" == "sab" : "'MC' -> 'SAB [%{8}]'"
            "%{2}" == "tsun" : "'MC' -> 'TPRN [%{8}]' -> 'TSUN [%{12}]'"
            default: "mc"
      version:
        transform: "0.1"
      addr:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=addr[=])\d+(?=[&]))/ => $1
      gui_str:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=gui[_]str[=]).+(?=[&]))/ => $1
      unit_str:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=unit[_]str[=])[a-zA-Z0-9\.]*)/ => $1
