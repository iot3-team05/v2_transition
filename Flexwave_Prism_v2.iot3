iot3: 2.0.0
info:
  name: '[Compact] CommScope Flexwave Prism'
  oem: CommScope
  model: Flexwave Prism
  versions:
    - "8.1"
  author: deep.desai@qeleo.com
  stage: alpha
  revision: 2
  labels:
    - compact
    - commscope
    - flexwave
    - prism
    - auto upgraded to v2
  created: "2022-02-28"
  updated: "2022-11-23"
  documentation:
    description: This IoT3 file supports alarms of CommScope Flexwave Prism systems.
registration:
  vaultId: vault_id
  fingerprint:
    - ip
    - port
    - vault_id
  vault:
    properties:
      password:
        key: password
        placeholder: Password
        required: true
        title: Password
        type: string
      username:
        key: username
        placeholder: Username
        required: true
        title: Username
        type: string
    required:
      - username
      - password
  form:
    properties:
      ip:
        key: ip
        placeholder: IP Address
        required: true
        title: IP
        type: string
      port:
        inputType: number
        key: port
        maximum: 65535
        minimum: 1
        placeholder: Port
        required: true
        title: Port
        type: number
      vault_id:
        key: vault_id
        pattern: ^(cu\w{6})(-av\w{6})$
        placeholder: Vault ID
        required: true
        title: Vault ID
        type: string
    required:
      - ip
      - port
      - vault_id
engines:
  tunnel:
    ssh:
      supported: false
    http:
      supported: true
  upgraded-http:
    schedule: '@always'
    auth: '%{resources.auths.upgraded-http}'
    type: HTTP
    options:
      timeout: 1m
      dialTimeout: 30s
      tlsHandshakeTimeout: 30s
      responseHeaderTimeout: 1m
      maxConnections: "5"
      tls:
        minVersion: "1.0"
topology:
  rootType: host
  wrapperType: system
  nodes: true
  discovery:
    dataEngine: '%{engines.upgraded-http}'
    dataSource: '%{resources.http.requests.get_info}'
    type: http
    expression: {}
    selectors:
      nodes:
        dataEngine: '%{engines.upgraded-http}'
        selector:
          type: xpath
          expression: //table[contains(@id,"swInventoryTable")]/tbody/tr
        extractor:
          filters:
            1: wrap <table> </table>
    flatten:
      type:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //tr/td[1]/text()
          2: toLower
          3: |
            switch
            "%{2}" ~= "host" : "host"
            "%{2}" ~= "pru" : "remote"
            default: ""
      topology:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //tr/td[1]/text()
          2: toLower
          3: use %{0}
          4: xpath //tr/td[3]/text()
          5: |
            switch
            "%{2}" ~= "host" : "host"
            "%{2}" ~= "pru" : "remote"
            default: ""
          6: |
            switch
            "%{5}" == "host" : "'host'"
            "%{5}" == "remote" : "'host' -> '%{4}'"
            default: "'host'"
      version:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //tr/td[4]
          2: regexpReplace /^(\d+\.\d+)\.?.*$/ => $1
      unit_id:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //tr/td[3]/text()
