iot3: 2.0.0
info:
  name: Adrf Fire
  oem: ADRF
  model: Fire
  versions:
    - "3.0"
  description: Adrf Fire [Alarm]
  author: bhagyashree.bothra@qeleo.com
  stage: alpha
  revision: 2
  labels:
    - adrf
    - fire
    - alarms
    - auto upgraded to v2
  created: "2022-05-23"
  updated: "2022-12-05"
registration:
  vaultId: vault_id
  fingerprint:
    - ip
    - port
    - vault_id
  vault:
    properties:
      password:
        key: password
        placeholder: Password
        required: true
        title: Password
        type: string
      username:
        key: username
        placeholder: Username
        required: true
        title: Username
        type: string
    required:
      - username
      - password
  form:
    properties:
      ip:
        key: ip
        placeholder: IP Address
        required: true
        title: IP
        type: string
      port:
        inputType: number
        key: port
        maximum: 65535
        minimum: 1
        placeholder: Port
        required: true
        title: Port
        type: number
      vault_id:
        key: vault_id
        pattern: ^(cu\w{6})(-av\w{6})$
        placeholder: Vault ID
        required: true
        title: Vault ID
        type: string
    required:
      - ip
      - port
      - vault_id
engines:
  tunnel:
    ssh:
      supported: false
    http:
      supported: true
  upgraded-http:
    schedule: '@always'
    auth: '%{resources.auths.upgraded-http}'
    type: HTTP
    options:
      tls:
        minVersion: "1.0"
topology:
  rootType: nms
  wrapperType: system
  nodes: true
  discovery:
    dataEngine: '%{engines.upgraded-http}'
    dataSource: '%{resources.http.requests.get_tree}'
    type: http
    selectors:
      nodes:
        dataEngine: '%{engines.upgraded-http}'
        selector:
          type: xslt
          expression: '%{resources.documents.flatten_tree}'
        extractor:
          filters:
            1: xpath //node
    flatten:
      type:
        filters:
          1: xpath //node/@type
          2: toLower
      topology:
        filters:
          1: xpath //node/@topology
          2: regexpReplace /(\[\d+\])([^;]+);/ => '"$2 $1" -> '
          3: regexpReplace /-> $/ => ''
      version:
        transform: "1.0"
      name:
        filters:
          1: xpath //node/@name
          2: use %{0}
          3: xpath //node/@comment
          4: |
            switch
            "%{3}" == "": "%{1}"
            default:      "%{3}"
      set_device_id:
        filters:
          1: xpath //node/@id
