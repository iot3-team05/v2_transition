              type: static
              expression: ""
            dataSink: '%{resources.globals.auth.sessionId}'
  mappers:
    alarm:
      keyType: String
      valueType: String
      map:
        AAI_FAIL: Critical
        AAI_NORMAL: Normal
        HARD: Severe
        INACTIVE: Normal
        NORMAL: Normal
        SOFT: Warning
  globals:
    auth:
      sessionId: 0
    reg:
      ip: ${registration.form.properties.ip}
      password: ${registration.vault.properties.password}
      port: ${registration.form.properties.port}
      url: http://${registration.form.properties.ip}:${registration.form.properties.port}
      username: ${registration.vault.properties.username}
  http:
    requests:
      get_tree:
        method: POST
        uri: ${resources.globals.reg.url}/cgi-bin/request.cgi
        requestContentType: application/x-www-form-urlencoded
        responseContentType: text/xml
        query:
          ?cascadecode: ""
          ?version: 1.0.61
          ipaddress: ""
        body:
          ?USER_ID: ${resources.globals.reg.username}
          DEVICE_ID: ${topology.discovery.flatten.set_device_id}
          MAIN_COMMAND: null
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      login:
        method: POST
        uri: ${resources.globals.reg.url}/functions/setFormData.php
        requestContentType: application/x-www-form-urlencoded
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ?version: 1.0.61
          ipaddress: ""
        body:
          password: ${resources.globals.reg.password}
          submit_login: Login
          username: ${resources.globals.reg.username}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      logout:
        uri: ${resources.globals.reg.url}/NMS/status.php
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ?version: 1.0.61
          ipaddress: ""
          logout: ""
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
