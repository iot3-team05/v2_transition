resources:
  auths:
    upgraded-http:
      http:
        httpUri: false
        actions:
          login:
            dataEngine: '%{engines.upgraded-http}'
            dataSource: '%{resources.http.requests.login}'
            selector:
              type: cookie
              expression: PHPSESSID
            dataSink: '%{resources.globals.auth.sessionId}'
          logout:
            dataEngine: '%{engines.upgraded-http}'
            dataSource: '%{resources.http.requests.logout}'
            selector:
              type: static
              expression: ""
            dataSink: '%{resources.globals.auth.sessionId}'
  mappers:
    alarms:
      keyType: String
      valueType: String
      map:
        gray: Link Fail
        green: Normal
        red: Critical
        yellow: Major
  globals:
    auth:
      sessionId: 0
    reg:
      ip: ${registration.form.properties.ip}
      password: ${registration.vault.properties.password}
      port: ${registration.form.properties.port}
      url: http://${registration.form.properties.ip}:${registration.form.properties.port}
      username: ${registration.vault.properties.username}
  http:
    requests:
      get_status:
        uri: ${resources.globals.reg.url}/status.php
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ipaddress: ""
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      get_tree:
        uri: ${resources.globals.reg.url}/status.php
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ipaddress: ""
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      login:
        method: POST
        uri: ${resources.globals.reg.url}/modules/setFormData.php
        requestContentType: application/x-www-form-urlencoded
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ipaddress: ""
        body:
          password: ${resources.globals.reg.password}
          submit_login: Login
          username: ${resources.globals.reg.username}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      logout:
        uri: ${resources.globals.reg.url}/status.php
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ipaddress: ""
          logout: ""
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
      set_form_data:
        method: POST
        uri: ${resources.globals.reg.url}/modules/setFormData.php
        requestContentType: application/x-www-form-urlencoded
        responseContentType: text/html
        query:
          ?cascadecode: ""
          ipaddress: ""
        body:
          newID: ${topology.discovery.flatten.device_name}
          submit_newID: 1
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: Status-Code
              match: ^(?!200)
