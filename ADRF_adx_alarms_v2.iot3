iot3: 2.0.0
info:
  name: Adrf Adx
  oem: ADRF
  model: ADX
  versions:
    - "3.0"
  description: Adrf Adxv
  author: deep.desai@qeleo.com
  stage: alpha
  revision: 2
  labels:
    - auto upgraded to v2
  created: "2022-05-23"
  updated: "2022-12-05"
registration:
  vaultId: vault_id
  fingerprint:
    - ip
    - port
    - vault_id
  vault:
    properties:
      password:
        key: password
        placeholder: Password
        required: true
        title: Password
        type: string
      username:
        key: username
        placeholder: Username
        required: true
        title: Username
        type: string
    required:
      - username
      - password
  form:
    properties:
      ip:
        key: ip
        placeholder: IP Address
        required: true
        title: IP
        type: string
      port:
        inputType: number
        key: port
        maximum: 65535
        minimum: 1
        placeholder: Port
        required: true
        title: Port
        type: number
      vault_id:
        key: vault_id
        pattern: ^(cu\w{6})(-av\w{6})$
        placeholder: Vault ID
        required: true
        title: Vault ID
        type: string
    required:
      - ip
      - port
      - vault_id
engines:
  tunnel:
    ssh:
      supported: false
    http:
      supported: true
  upgraded-http:
    schedule: '@always'
    auth: '%{resources.auths.upgraded-http}'
    type: HTTP
    options:
      tls:
        minVersion: "1.0"
topology:
  rootType: nms
  wrapperType: system
  nodes: true
  discovery:
    dataEngine: '%{engines.upgraded-http}'
    dataSource: '%{resources.http.requests.get_tree}'
    type: http
    selectors:
      nodes:
        dataEngine: '%{engines.upgraded-http}'
        selector:
          type: xpath
          expression: //*[@name="targetTree"]//*[contains(@class,"btnViewTarget")]/..
        extractor:
          filters:
            1: wrap <table> </table>
    flatten:
      type:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: "switch\n\"%{0}\" ~= \"NMS\" : \"nms\"\n\"%{0}\" ~= \"RFU\" : \"rfu\"\n\"%{0}\" ~= \"ODU\" : \"odu\"\n\"%{0}\" ~= \"RU-Hub\" : \"ru_hub\"\n\"%{0}\" ~= \"M-RU\" : \"m_ru\"\ndefault: \"\"    \n"
      topology:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=01[_]01[_])[A-Z0-9_]+(?=[&]))/ => $1
          2: regexpExtract /(^\d+)/ => $1
          3: use %{1}
          4: regexpExtract /((?<=0F_)\d+(?=[_]))/ => $1
          5: use %{1}
          6: regexpExtract /((?=\d+)\d+(?=[_]\d+$))/ => $1
          7: use %{1}
          8: regexpExtract /(\d+$)/ => $1
          9: use %{0}
          10: xpath //td/text()
          11: |
            switch
            "%{10}" ~= "NMS" : "nms"
            "%{10}" ~= "RFU" : "rfu"
            "%{10}" ~= "ODU" : "odu"
            "%{10}" ~= "RU-Hub" : "ru_hub"
            "%{10}" ~= "M-RU" : "m_ru"
            default: ""
          12: |
            switch
            "%{11}" == "nms" : "'nms'"
            "%{11}" == "rfu" : "'nms' -> 'rfu [%{2}]'"
            "%{11}" == "odu" : "'nms' -> 'odu [%{4}]'"
            "%{11}" == "ru_hub" : "'nms' -> 'odu [%{4}]' -> 'ru_hub [%{6}]'"
            "%{11}" == "m_ru" : "'nms' -> 'odu [%{4}]' -> 'ru_hub [%{6}]' -> 'm_ru[33] [%{8}]'"
            default: ""
      version:
        transform: "1.0"
      device_name:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: regexpExtract /((?<=&#39;)[A-Z0-9_]+(?=&#39))/ => $1
