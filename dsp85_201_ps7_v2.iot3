resources:
  auths:
    upgraded-http:
      http:
        httpUri: false
        method: Basic
        properties:
          password: ${registration.vault.properties.password}
          username: ${registration.vault.properties.username}
  mappers:
    alarm_severity:
      keyType: String
      valueType: String
      map:
        alarmClear: Normal
        alarmDisabled: Disabled
        alarmSet: Critical
    gain:
      keyType: Int32
      valueType: String
      map:
        "0": Neither Link; Leave Unchanged
        "1": Uplink Only
        "3": Uplink and Downlink
    on_off:
      keyType: Int32
      valueType: String
      map:
        "0": "Off"
        "1": "On"
  globals:
    reg:
      ip: ${registration.form.properties.ip}
      password: ${registration.vault.properties.password}
      port: ${registration.form.properties.port}
      url: http://${registration.form.properties.ip}:${registration.form.properties.port}
      username: ${registration.vault.properties.username}
  http:
    requests:
      get_index:
        uri: ${resources.globals.reg.url}/index.cgi
        requestContentType: gzip
        responseContentType: text/html
        body:
          ga:
            - ${global.auth.sessionID}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "500"
      get_local:
        uri: ${resources.globals.reg.url}/netconf.cgi
        requestContentType: gzip
        responseContentType: text/html
        header:
          ga:
            - ${resources.globals.auth.sessionID}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "500"
      get_remote:
        uri: ${resources.globals.reg.url}/remotenetconf.cgi
        requestContentType: gzip
        responseContentType: text/html
        header:
          ga:
            - ${resources.globals.auth.sessionID}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "500"
      get_rf:
        uri: ${resources.globals.reg.url}/rf.cgi
        requestContentType: gzip
        responseContentType: text/html
        header:
          ga:
            - ${resources.globals.auth.sessionID}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "500"
      get_status:
        uri: ${resources.globals.reg.url}/status.cgi
        requestContentType: gzip
        responseContentType: text/html
        header:
          ga:
            - ${resources.globals.auth.sessionID}
        responses:
          success:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "200"
          failure:
            - dataSource: '%{response.header}'
              expression: status-code
              match: "500"
