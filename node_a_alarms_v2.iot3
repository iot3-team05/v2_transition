iot3: 2.0.0
info:
  name: '[Compact] Commscope Node A'
  oem: Commscope
  model: Node A
  versions:
    - "2.0"
  author: Fatema Nevtiwala <support@qeleo.com>
  stage: alpha
  revision: 2
  labels:
    - compact
    - commscope
    - node a
    - auto upgraded to v2
  created: "2022-06-17"
  updated: "2022-11-23"
  documentation:
    description: This IoT3 file supports the alarms of Commscope Node A systems.
registration:
  vaultId: vault_id
  fingerprint:
    - ip
    - port
    - vault_id
  vault:
    properties:
      password:
        key: password
        placeholder: Password
        required: true
        title: Password
        type: string
      username:
        key: username
        placeholder: Username
        required: true
        title: Username
        type: string
    required:
      - username
      - password
  form:
    properties:
      ip:
        key: ip
        placeholder: IP Address
        required: true
        title: IP
        type: string
      port:
        inputType: number
        key: port
        maximum: 65535
        minimum: 0
        placeholder: Port
        required: true
        title: Port
        type: number
      vault_id:
        key: vault_id
        pattern: ^(cu\w{6})(-av\w{6})$
        placeholder: Vault ID
        required: true
        title: Vault ID
        type: string
    required:
      - ip
      - port
      - vault_id
engines:
  tunnel:
    ssh:
      supported: false
    http:
      supported: true
  upgraded-http:
    schedule: '@always'
    auth: '%{resources.auths.upgraded-http}'
    type: HTTP
    options:
      tls:
        minVersion: "1.0"
topology:
  rootType: system
  nodes: true
  discovery:
    dataEngine: '%{engines.upgraded-http}'
    dataSource: '%{resources.http.requests.get_summary}'
    type: http
    selectors:
      nodes:
        dataEngine: '%{engines.upgraded-http}'
        selector:
          type:  xpath
          expression: //h3[text() = "Slot Status"]/following-sibling::table[1]/tbody/tr[boolean(td[1]/h5)]
        extractor:
          filters:
            1: wrap <tr> </tr>
            2: wrap <table> </table>
    flatten:
      type:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //td[1]/h5/text()
          2: regexpExtract /^\d/ => $0
          3: use ${0}
          4: xpath //td[4]/h5/text()
          5: |
            switch
            "%{4}" == "Disabled" : "0"
            default: "%{2}"
        mapper: '%{resources.mappers.type}'
      topology:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //td[1]/h5/text()
          2: regexpExtract /^\d/ => $0
          3: use ${0}
          4: xpath //td[4]/h5/text()
          5: |
            switch
            "%{4}" == "Disabled" : "0"
            default: "%{2}"
          6: mapper %{resources.mappers.type}
      version:
        transform: "1.0"
      slot:
        transform: ${topology.discovery.selectors.nodes}
        filters:
          1: xpath //td[1]/h5/text()
          2: regexpExtract /^\d/ => $0
