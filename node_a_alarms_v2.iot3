resources:
  auths:
    upgraded-http:
      http:
        httpUri: false
        actions:
          login:
            dataEngine: '%{engines.upgraded-http}'
            dataSource: '%{resources.http.requests.login}'
            requires:
              - session_id
            selector:
              type: http
            lookup:
              session_id:
                dataEngine: '%{engines.upgraded-http}'
                dataSource: '%{resources.http.requests.get_session_id}'
                selector:
                  type:  
                  expression: //input[@id="sessionId"]/@value
                dataSink: '%{resources.globals.auth.sessionId}'
          logout:
            dataEngine: '%{engines.upgraded-http}'
            dataSource: '%{resources.http.requests.logout}'
            selector:
              type: static
              expression: ""
            dataSink: '%{resources.globals.auth.sessionId}'
  mappers:
    alarm_severity:
      keyType: String
      valueType: String
      map:
        aqua: Warning
        gray: Normal
        lime: Normal
        orange: Critical
        red: Severe
        yellow: Minor
    type:
      keyType: Int32
      valueType: String
      map:
        "1": slot1
        "2": slot2
        "3": slot3
        "4": slot4
  globals:
    auth:
      sessionId: ""
    reg:
      ip: ${registration.form.properties.ip}
      password: ${registration.vault.properties.password}
      port: ${registration.form.properties.port}
      url: http://${registration.form.properties.ip}:${registration.form.properties.port}
      username: ${registration.vault.properties.username}
  http:
    requests:
      get_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/alarms.cgi
        query:
          from: home.cgi
          sessionId_1: ${resources.globals.auth.sessionId}
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //table[2]/tbody/tr/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_external_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/extAlm.cgi
        query:
          sessionId_ext: ${resources.globals.auth.sessionId}
          summary_ext: "1"
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //form[@id="alm_form"]/table/tbody/tr/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_main_board_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/mainBoardAlm.cgi
        query:
          sessionId_main: ${resources.globals.auth.sessionId}
          summary_main: "1"
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //form[@id="alm_form"]/table/tbody/tr/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_psu_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/psuAlm.cgi
        query:
          sessionId_psu: ${resources.globals.auth.sessionId}
          summary_psu: "1"
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //form[@id="alm_form"]/table/tbody/tr/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_psu_ventilation_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/ventilationAlm.cgi
        query:
          sessionId_ventilation: ${resources.globals.auth.sessionId}
          summary: "1"
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //table[2]/tbody/tr[1]/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_session_id:
        uri: ${resources.globals.reg.url}/cgi-bin/login.cgi
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //input[@id="sessionId"]/@value
              match: ^.+$
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //h3[@style="color:red"]/text()
              match: Login incorrect please try again
      get_slots_alarms:
        uri: ${resources.globals.reg.url}/cgi-bin/modAlm.cgi
        query:
          modnum: ${topology.discovery.flatten.slot}
          sessionId_mod: ${resources.globals.auth.sessionId}
          summary_mod: "1"
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //form[@id="alm_form"]/table/tbody/tr/td[1]/h4/text()
              match: Picture
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      get_summary:
        uri: ${resources.globals.reg.url}/cgi-bin/summary.cgi
        query:
          from: status.cgi
          sessionId_1: ${resources.globals.auth.sessionId}
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //title//text()
              match: Summary
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="login"]/text()
              match: Login
      login:
        method: POST
        uri: ${resources.globals.reg.url}/cgi-bin/handleLogin.cgi
        requestContentType: application/x-www-form-urlencoded
        body:
          ie: "0"
          pwd: ${resources.globals.reg.password}
          sessionId: ${resources.globals.auth.sessionId}
          submit: Submit
          user: ${resources.globals.reg.username}
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="logout"]/text()
              match: Logout
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: //h3[@style="color:red"]/text()
              match: Login incorrect please try again
      logout:
        uri: ${resources.globals.reg.url}/cgi-bin/logout.cgi
        query:
          sessionId: ${resources.globals.auth.sessionId}
        responses:
          success:
            - dataSource: '%{response.body}'
              type:  
              expression: //button[@id="Login"]/text()
              match: Login
          failure:
            - dataSource: '%{response.body}'
              type:  
              expression: boolean(//button[@id="Login"])
              match: "false"
